#include<windows.h>
#include <GL/glut.h>
#include <stdlib.h>
#include <stdio.h>


void Myinit(void);
void Display(void);


main(int argc, char** argv)
{
	glutInit(&argc, argv);
	glutInitDisplayMode(GLUT_SINGLE| GLUT_RGB);
	glutInitWindowSize(600,480);
	glutInitWindowPosition(350,150);
	glutCreateWindow("MALL");
	glutDisplayFunc(Display);
	myInit();
	glutMainLoop();
}

void Myinit(void){

  glClearColor(1.0,1.0,1.0,0.0);
  glColor3f(0.0f, 0.0f, 0.0f);
 	glPointSize(4.0);
	glMatrixMode(GL_PROJECTION);
	glLoadIdentity();
	gluOrtho2D(0.0, 20.0, 0.0, 20.0);

}
void Display(void){


}
